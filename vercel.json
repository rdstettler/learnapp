{
    "version": 2,
    "installCommand": "npm install && cd frontend && npm install",
    "buildCommand": "cd frontend && npm run build",
    "devCommand": "cd frontend && ng serve --port $PORT",
    "outputDirectory": "frontend/dist/learnapp/browser",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ]
}